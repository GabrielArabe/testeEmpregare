        <div class="col-3"></div>
        <div class="col-6">
            <h1>Cadastro de usuário</h1>
        </div>
        <div class="col-3"></div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <label>Nome: </label>
            <input type="text" id="nome" value="" />
        </div>

        <div class="col-md-6">
            <label>Telefone: </label>
            <input type="tel" id="telefone" value="" />
