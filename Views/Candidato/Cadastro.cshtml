        <div class="col-md-6">
            <label>E-mail: </label>
            <input type="email" id="email" value="" />
        </div>

        <div class="col-md-6">
            <label>Senha: </label>
            <input type="password" id="senha"  value="" />
        </div>
    </div>
       
        <button class="btn-primary" value="Cadastrar" onclick="cadastrarCandidato()">Cadastrar</button>
</div>